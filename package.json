{
  "name": "vue-three-template",
  "description": "vue 3.x版本项目模板,vite 4.x打包构建",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "prettier": "prettier --cache --check **/**/*.{ts,js,vue}",
    "prettier:fix": "prettier --cache --write **/**/*.{ts,js,vue}",
    "stylelint": "stylelint **/*.{scss,.vue} --cache",
    "stylelint:fix": "stylelint **/*.{scss,.vue} --fix --cache",
    "eslint": "eslint src --cache --ext .js,.jsx,.ts,.tsx,.vue",
    "eslint:fix": "eslint src --fix --cache --ext  .js,.jsx,.ts,.tsx,.vue",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.0.10",
    "axios": "^1.2.2",
    "element-plus": "^2.2.28",
    "husky": "^8.0.3",
    "pinia": "^2.0.28",
    "postcss": "^8.4.20",
    "postcss-html": "^1.5.0",
    "sass": "^1.57.1",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.0",
    "@commitlint/config-conventional": "^17.4.0",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/node": "^18.11.12",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "stylelint": "^14.16.1",
    "stylelint-config-recess-order": "^3.1.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.3.0",
    "terser": "^5.16.1",
    "typescript": "~4.7.4",
    "unplugin-auto-import": "^0.12.1",
    "unplugin-vue-components": "^0.22.12",
    "vite": "^4.0.0",
    "vue-tsc": "^1.0.12"
  }
}
